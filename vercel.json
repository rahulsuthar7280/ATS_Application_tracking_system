{
  "builds": [
    {
      "src": "Application_tracking_system/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Application_tracking_system/wsgi.py"
    }
  ]
}
